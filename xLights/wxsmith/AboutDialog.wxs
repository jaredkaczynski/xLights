									<label>Privacy Policy</label>
									<url>https://xlights.org/privacy-policy/</url>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxHyperlinkCtrl" name="ID_HYPERLINKCTRL2" variable="EULAHyperlinkCtrl" member="yes">
